{
  "mcpServers": {
    "sentry": {
      "type": "stdio",
      "command": "bash",
      "args": ["-c", "npx @sentry/mcp-server@latest --access-token $(op read 'op://Private/Sentry MCP/credential')"],
      "env": {}
    },
    "postgresql": {
      "type": "stdio",
      "command": "bash",
      "args": ["-c", "npx @modelcontextprotocol/server-postgres $(op read 'op://Private/Crunchy Bridge Labelfish/connection string')"],
      "env": {}
    },
    "github": {
      "type": "stdio",
      "command": "npx",
      "args": ["@modelcontextprotocol/server-github"],
      "env": {}
    }
  }
}
