  "validation_rules": {
    "IMO": {
      "format": "7 digits",
      "validation": "Luhn checksum"
    },
    "MMSI": {
      "format": "9 digits",
      "prefix_validation": {
        "2xx": "Europe",
        "3xx": "North America",
        "4xx": "Asia",
        "5xx": "Oceania",
        "6xx": "Africa",
        "7xx": "South America"
      }
    },
    "EU_CFR": {
      "format": "XXX000000000",
      "description": "3 letters (country) + 9 digits"
    },
    "IRCS": {
      "format": "4-8 alphanumeric",
      "case": "uppercase"
    }
  },

  "edge_cases": [
    {
      "id": "edge_imo_01",
      "text": "The vessel reports IMO APPLIED NOT YET RECEIVED status.",
      "expected_entities": [
        {"text": "APPLIED NOT YET RECEIVED", "label": "O", "note": "Special status, not an IMO number"}
      ]
    },
    {
      "id": "edge_multiple_names_01",
      "text": "The vessel OCEAN STAR formerly known as SEA EAGLE (ex-WAVE RIDER)",
      "expected_entities": [
        {"text": "OCEAN STAR", "label": "VESSEL_NAME", "start": 11, "end": 21},
        {"text": "SEA EAGLE", "label": "VESSEL_NAME", "start": 41, "end": 50},
        {"text": "WAVE RIDER", "label": "VESSEL_NAME", "start": 55, "end": 65}
      ]
    },
    {
      "id": "edge_flag_change_01",
      "text": "The vessel changed flag from Liberia to Panama in March 2024.",
      "expected_entities": [
        {"text": "Liberia", "label": "FLAG", "start": 30, "end": 37},
        {"text": "Panama", "label": "FLAG", "start": 41, "end": 47},
        {"text": "March 2024", "label": "DATE", "start": 51, "end": 61}
      ]
    }
  ],

  "evaluation_metrics": {
    "exact_match": "Entity boundaries must match exactly",
    "partial_match": "At least 50% overlap with ground truth",
    "label_accuracy": "Correct entity type classification",
    "metrics_to_track": [
      "precision",
      "recall",
      "f1_score",
      "entity_level_accuracy",
      "token_level_accuracy"
    ]
